<App>
 <Page title="{i18n>homePageTitle}">
  <content>
   <VBox fitContainer="true" justifyContent="Center" alignItems="Center" alignContent="Center">
    <items>
     <Input id="username" placeholder="User ID"></Input>
     <Input id="password" placeholder="Password" type="Password"></Input>
     <Button width="12rem" text="Login" type="Emphasized" press="onValidation"></Button>
    </items>
   </VBox>
  </content>
  <footer >
   <Bar>
    <contentleft>
     <Text text="techippo"></Text>
    </contentLeft>
    <contentright>
     <Text text="myLogin"></Text>
    </contentRight>
   </Bar>
  </footer>
 </Page>
</App>